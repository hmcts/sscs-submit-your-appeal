{
  "node": {
    "protocol": "http",
    "hostname": "localhost",
    "port": 3000
  },
  "redis": {
    "url": "redis://127.0.0.1:6379",
    "secret": "defaultsecret",
    "useSSL": "false",
    "timeout": 1200
  },
  "frontend": {
    "inactivityAlert": 1080
  },
  "api": {
    "url":  "http://localhost:8080",
    "uploadEvidenceUrl": "http://localhost:3010/upload"
  },
  "appInsights": {
    "instrumentationKey": "iKey"
  },
  "ssl": {
    "hpkp": {
      "maxAge": "3600",
      "sha256s": "AbCdEf123=",
      "sha256sBackup": "ZyXwVu456="
    }
  },
  "e2e": {
    "waitForTimeout": "1000",
    "waitForAction": "500",
    "outputDir": "./functional-output",
    "frontendUrl": "http://localhost:3000"
  },
  "features": {
    "evidenceUpload": {
      "enabled": false,
      "maxFileSize": 10
    }
  },
  "postcodeChecker": {
    "endpoint": "/regionalcentre",
    "enabled": false,
    "allowedRpcs": "birmingham"
  }
}
