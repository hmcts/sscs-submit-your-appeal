{
  "name": "sya",
  "description": "Submit Your Appeal",
  "version": "0.0.1",
  "license": "MIT",
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "start": "cross-env NODE_PATH=. node server.js",
    "build": "webpack --config ./webpack/webpack.prod.js",
    "dev": "cross-env NODE_ENV=development NODE_PATH=.  node server.js",
    "debug": "NODE_ENV=development cross-env NODE_PATH=. node --inspect server.js",
    "setup": "exit 0",
    "mock": "yarn mock-uploader & yarn debug",
    "test:unit": "cross-env NODE_PATH=. NODE_ENV=test mocha --exit 'test/unit/**/*.test.js'",
    "test:assets": "cross-env NODE_PATH=. NODE_ENV=test mochapack 'test/assets/**/*.test.js' --webpack-config test/webpack.config.js --require babel-core/register",
    "test": "yarn test:unit && yarn test:assets",
    "test:all": "cross-env NODE_PATH=. NODE_ENV=test yarn test && yarn test:assets && yarn test:audit && yarn test:smoke",
    "lint": "cross-env NODE_PATH=. eslint --fix .",
    "test:smoke": "cross-env NODE_PATH=. NODE_ENV=test codeceptjs run -c test/e2e/ --grep @smoke --steps --verbose",
    "test:evidence-upload": "cross-env NODE_PATH=. NODE_ENV=test codeceptjs run -c test/e2e/ --grep @evidence-upload --steps",
    "test:functional": "cross-env NODE_PATH=. NODE_ENV=test codeceptjs run -c test/e2e/ --grep @functional --steps --verbose",
    "test:e2e-pages": "cross-env NODE_PATH=. NODE_ENV=test codeceptjs run-multiple pages -c test/e2e/ --steps --reporter mocha-multi",
    "test:functional:batch": "cross-env NODE_PATH=. NODE_ENV=test codeceptjs run -c test/e2e/ --grep @batch-$BATCH --steps",
    "test:functional:batches": "for i in `seq -w 1 12`; do BATCH=$i yarn test:functional:batch; [ $? -ne 0 ] && exit $?; done; exit 0;",
    "test:coverage": "cross-env NODE_PATH=. NODE_ENV=test istanbul cover _mocha 'test/unit/**/*.test.js' --dir test/unit/coverage/html",
    "test:crossbrowser": "cross-env NODE_PATH=. NODE_ENV=test codeceptjs run-multiple ${BROWSER_GROUP:-'--all'} -c test/e2e/saucelabs.conf.js --steps --reporter mocha-multi --verbose",
    "test:a11y": "cross-env NODE_PATH=. NODE_ENV=a11y LOG_LEVEL=ERROR mocha --exit test/accessibility/a11y.js --exit --timeout 60000 --reporter mochawesome --reporter-options reportFilename=a11y,inlineAssets=true,reportTitle=submit-your-appeal",
    "sonar-scan": "sonar-scanner -Dsonar.projectName='SSCS :: Submit Your Appeal frontend' -Dsonar.projectKey='SSCSSYAF' -Dsonar.sources=. -Dsonar.exclusions=app.js,appConfigurations.js,app-insights.js,server.js,config/**,charts/**,infrastructure/**,node_modules/**,mocks/**,policy-pages/**,test/**,assets/**,dist/**,mochawesome-report/**,webpack/**,components/postcodeLookup/assets/** -Dsonar.javascript.lcov.reportPaths=test/unit/coverage/html/lcov.info -Dsonar.cpd.exclusions=steps/**",
    "mock-uploader": "node test/file_acceptor/evidence_uploader.js",
    "mock-ccd": "node mocks/ccd/server.js"
  },
  "pre-commit": [
    "lint"
  ],
