{
  "name": "sya",
  "description": "Submit Your Appeal",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_PATH=. node server.js",
    "start-dev": "cross-env NODE_PATH=. node-dev server.js",
    "setup": "exit 0",
    "test": "cross-env NODE_PATH=. NODE_ENV=test nyc mocha 'test/unit/**/*.test.js'",
    "e2e": "cross-env NODE_PATH=. NODE_ENV=test codeceptjs run -c test/e2e/ --steps",
    "test-all": "cross-env NODE_PATH=. NODE_ENV=test yarn test && yarn test:nsp && yarn e2e",
    "lint": "echo 'linting not implemented'",
    "test:smoke": "echo 'smoke test not implemented'",
    "test:coverage": "echo 'test coverage not implemented'",
    "test:nsp": "nsp check",
    "test:a11y": "echo 'a11y tests not implemented'",
    "sonar-scanner": "node_modules/sonar-scanner/bin/sonar-scanner",
    "health-check": "echo 'not implemented yet'"
  },
  "dependencies": {
    "@hmcts/look-and-feel": "1.3.0",
    "@hmcts/nodejs-healthcheck": "^1.4.3",
    "@hmcts/one-per-page": "^2.0.0-3",
    "accessible-autocomplete": "^1.6.1",
    "body-parser": "^1.18.2",
    "cross-env": "^5.0.5",
    "express": "^4.15.3",
    "http-status-codes": "^1.3.0",
    "joi": "^10.6.0",
    "moment": "^2.18.1",
    "nsp": "^3.1.0",
    "proxyquire": "^1.8.0",
    "request-promise-native": "^1.0.5"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-jq": "^0.0.9",
    "codeceptjs": "^1.0.2",
    "istanbul": "^0.4.5",
    "lodash": "^4.17.4",
    "mocha": "^3.5.3",
    "nightmare": "^2.10.0",
    "nightmare-upload": "^0.1.1",
    "node-dev": "^3.1.3",
    "nyc": "^11.4.0",
    "portscanner": "^2.1.1",
    "sinon": "^3.2.1",
    "sinon-chai": "^2.13.0",
    "sonar-scanner": "^1.0.1",
    "supertest": "^3.0.0"
  },
  "nyc": {
    "extension": [
      ".js"
    ],
    "exclude": [
      "test/**","assets/js/**", "node_modules/**", "test/unit/coverage/**"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "report-dir": "test/unit/coverage",
    "all": true
  }
}
